{
  "name": "xss",
  "version": "2.0.0",
  "description": "Development tools",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun run clear && tsc && concurrently \"bun run build:html\" \"bun run build:background\" \"bun run build:content\"",
    "build:html": "cross-env BUILD_INPUT=html vite build",
    "build:background": "cross-env BUILD_INPUT=background vite build",
    "build:content": "cross-env BUILD_INPUT=content vite build",
    "preview": "vite preview",
    "clear": "del ./XSS",
    "lint": "eslint . && bun run lint:css",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/totofish/XSS.git"
  },
  "keywords": [
    "XSS"
  ],
  "author": "totofish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/totofish/XSS/issues"
  },
  "homepage": "https://github.com/totofish/XSS#readme",
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@uiw/codemirror-theme-vscode": "^4.23.7",
    "@uiw/react-codemirror": "^4.23.7",
    "js-base64": "^3.7.7",
    "js-beautify": "^1.15.1",
    "polished": "^4.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "styled-components": "^6.1.13",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@types/babel__core": "^7.20.5",
    "@types/chrome": "^0.0.287",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "del-cli": "^6.0.0",
