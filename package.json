    "globals": "^15.14.0",
    "postcss-styled-syntax": "^0.7.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.0",
    "stylelint": "^16.12.0",
    "stylelint-config-recommended": "^14.0.1",
    "terser": "^5.37.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
